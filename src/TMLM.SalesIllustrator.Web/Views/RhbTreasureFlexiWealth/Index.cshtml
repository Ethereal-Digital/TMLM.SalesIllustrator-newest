<body>
    <section class="page">
        <div id="objectLoader" class="loader-wrapper">
            <div id="loader" class="hidden"></div>
        </div>
        <div class="d-block">
            <section>
                <div class="header_bar">
                    <img style="width: 10%;" src="~/assets/CoreLogo.png">
                    <div class="header">Sales Illustrator</div>
                </div>
                <div class="top" style="padding-top: 20px;">
                    <div class="pagination_button" id="pagination">
                        <div style="width: 100%; display: flex; justify-content: space-between;">
                            <div style="margin-right: 0.5rem; display: flex;">
                                <button class="back_button" onclick="back_page()">
                                    Back
                                </button>
                            </div>


                            <div style="display: flex;">
                                <button class="home_button" onclick="home()">
                                    Home Page
                                </button>
                                <div>
                                    <button class="next_button" onclick="next_page()">
                                        Next
                                    </button>
                                    <br id="break">
                                    @* <button class="skip_button" style="margin-top: 10px;" onclick="skip()">
                                        Skip
                                    </button> *@
                                </div>


                            </div>
                        </div>
                    </div>

                    <div id="main_title" class="main-title center">
                        Choose Your Preferred Plan
                    </div>
                    <div id="sub_title" class="sub-title center">
                    </div>
                </div>
            </section>

            <section class="page_section fade-in" id="section_1">

                <div class="bottom">
                    <div style="width: 350px" onclick="next(2)" class="zoom">
                        <img class="mt-2" style="
                            display: block;
                            margin-left: auto;
                            cursor: pointer;
                            margin-right: auto; width: 100%" src="~/assets/main3.jpg">
                        <div class="main-button">
                            RHB Treasure Builder
                        </div>
                    </div>
                </div>
            </section>


            @* question *@

            <section class="page_section" id="section_19">
                <div class="bottom mt-2" style="align-items: flex-start; height: auto">
                    <div class="d-block enquiry_1">
                        <div class="d-block white">
                            <div class="address_name slide-in-bottom information_1">
                                <div class="col-12">
                                    <h4>How do I address you?</h4>
                                </div>
                                <div class="d-flex">
                                    <div class="col-12 px-0">
                                        <div class="d-flex m-auto">
                                            <div class="main-selection w_100">
                                                <input type="text"
                                                       onkeyup="numberMobile(event)"
                                                       pattern="[A-Za-z]" class="name"
                                                       style="background: transparent; border: 0;  text-align: center;  width: 100%; height: 100%;">
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="address_name slide-in-bottom information_2">
                                <div class="col-12">
                                    <h4>When were you born?</h4>
                                </div>
                                <div class="d-flex">
                                    <div class="col-12 px-0">
                                        <div class="d-flex m-auto">

                                            <!-- isNonMobile -->
                                            <div class=" main-selection w_100">
                                                <input onchange="validateDate(event)" type="date" id="age" name="age" style="background: transparent; border: 0;  text-align: center;    width: 100%; height: 100%;">
                                                <!--  min="1995-01-01" max="2000-12-31" -->
                                            </div>
                                        </div>
                                        <small class="hidden red invalid_date">Invalid date</small>
                                    </div>
                                </div>
                            </div>
                            <div class="address_name slide-in-bottom information_3">
                                <div class="col-12">
                                    <h4>What is your gender?</h4>
                                </div>
                                <div class="kids hide">
                                    <div class="col-6 center pointer">
                                        <img width="150" onclick="submitForm1('male', 'assets/images/Boy icon.png')" src="~/assets/images/Boy icon.png">
                                    </div>
                                    <div class="col-6 center pointer">
                                        <img width="150" onclick="submitForm1('female', 'assets/images/Girl icon.png')" src="~/assets/images/Girl icon.png">
                                    </div>
                                </div>
                                <div class="adult hide">
                                    <div class="col-6 center pointer">
                                        <img width="150" onclick="submitForm1('male', 'assets/images/Male icon.png')" src="~/assets/images/Male icon.png">
                                    </div>
                                    <div class="col-6 center pointer">
                                        <img width="150" onclick="submitForm1('female', 'assets/images/Female icon.png')" src="~/assets/images/Female icon.png">
                                    </div>
                                </div>
                                <div class="old hide">
                                    <div class="col-6 center pointer">
                                        <img width="150" onclick="submitForm1('male', 'assets/images/Grandpa icon.png')" src="~/assets/images/Grandpa icon.png">
                                    </div>
                                    <div class="col-6 center pointer">
                                        <img width="150" onclick="submitForm1('female', 'assets/images/Grandma icon.png')" src="~/assets/images/Grandma icon.png">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            <section class="page_section" id="section_2">
                <div class="bottom mt-2" style="align-items: flex-start; height: auto">
                    <div class="d-block enquiry_1">
                        <div class="d-block white">
